#include <stdio.h>

int main() {
    int num, digit, count = 0, sum = 0, product = 1;
    int temp;
    printf("Enter a number: ");
    scanf("%d", &num);
    temp = num;  
    while (temp > 0) {
        digit = temp % 10;     
        count++;               
        sum += digit;         
        product *= digit;      
        temp /= 10;            
    }
    printf("Number: %d\n", num);
    printf("Count of digits = %d\n", count);
    printf("Sum of digits = %d\n", sum);
    printf("Product of digits = %d\n", product);

    return 0;
}