#include <stdio.h>
int main() {
    int marks;
    printf("Enter your marks (0 - 100): ");
    scanf("%d", &marks);
    if (marks >= 75 && marks <= 100) {
        printf("Grade: A\n");
    } else if (marks >= 60 && marks < 75) {
        printf("Grade: B\n");
    } else if (marks >= 50 && marks < 60) {
        printf("Grade: C\n");
    } else if (marks >= 0 && marks < 50) {
        printf("Grade: Fail\n");
    } else {
        printf("Invalid marks! Please enter between 0 and 100.\n");
    }

    return 0;
}